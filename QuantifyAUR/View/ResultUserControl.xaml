<Window
    x:Class="QuantifyAUR.ResultWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    x:Name="ResultUC"
    Width="400">




    <ListView Name="lvResult" Margin="10">
        <ListView.View>
            <GridView>
                <GridViewColumn
                    Width="120"
                    DisplayMemberBinding="{Binding Alias}"
                    Header="Alias" />
                <GridViewColumn
                    Width="120"
                    DisplayMemberBinding="{Binding SumUnitQuantity}"
                    Header="Sum Unit Quantity" />
            </GridView>
        </ListView.View>
    </ListView>
</Window>