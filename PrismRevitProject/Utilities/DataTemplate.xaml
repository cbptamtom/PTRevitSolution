<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:view="clr-namespace:PrismRevitProject.Views"
    xmlns:vm="clr-namespace:PrismRevitProject.ViewModels">

    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:Home />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SettingViewModel}">
        <view:Setting />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DataAnalysisViewModel}">
        <view:DataAnalysis />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:GeometryViewModel}">
        <view:Geometry />
    </DataTemplate>




</ResourceDictionary>